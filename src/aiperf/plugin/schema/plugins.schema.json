          "description": {
            "description": "Human-readable description of this plugin type.",
            "title": "Description",
            "type": "string"
          },
          "priority": {
            "default": 0,
            "description": "Priority for conflict resolution (higher wins).",
            "title": "Priority",
            "type": "integer"
