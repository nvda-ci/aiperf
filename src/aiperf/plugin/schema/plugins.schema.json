                  "service_kind": {
                    "default": "openai",
                    "description": "The service kind of the endpoint (used for artifact naming).",
                    "title": "Service Kind",
                    "type": "string"
                  },
                  "supports_streaming": {
                    "default": false,
                    "description": "Whether endpoint supports streaming responses.",
